/*
   MPE Logging's generated header file for fixed size integer types
   and their printf format specifiers used in CLOG2.
*/
#if !defined( _CLOG_INTTYPES )
#define  _CLOG_INTTYPES

@mpe_c99_stdint_inc@
@mpe_c99_pridxx_inc@

typedef @mpe_int8_t@         CLOG_int8_t;
typedef @mpe_int16_t@        CLOG_int16_t;
typedef @mpe_int32_t@        CLOG_int32_t;
typedef @mpe_int64_t@        CLOG_int64_t;

/* 
   Define address-sized integer for the use of MPI_Comm_xxx_attr
   in clog2_commset.c.
*/
typedef @CLOG_PINT@          CLOG_Pint;

#define i8fmt        @mpe_int8_fmt@
#define i16fmt       @mpe_int16_fmt@
#define i32fmt       @mpe_int32_fmt@
#define i64fmt       @mpe_int64_fmt@

#endif
